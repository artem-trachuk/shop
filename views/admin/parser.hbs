<div class="row">
    <div class="col-12 col-lg-3">
        {{> adminNavigation}}
    </div>
    <div class="col-12 col-lg-6">
        <h2>.xlsx парсер Инфотех</h2>
        <hr>
        <form action="/admin/parser?_csrf={{csrfToken}}" method="POST" enctype="multipart/form-data">
        <input type="hidden" name="_csrf" value="{{ csrfToken }}">
            <div class="form-group">
                <label for="productImage">Выберите файл <span class="text-success font-weight-bold">Price_Infotech_Partner.xlsx</span></label>
                <input type="file" class="form-control-file" id="xlsx" name="xlsx">
            </div>
            <button class="btn btn-success" type="submit">Отправить</button>
        </form>
    </div>
</div>